Анимация квадрат уходит вправо 
// let x = 0;

function animate() {
  // очищаем канвас
  ctx.clearRect(0, 0, CANVAS_WIDTH, CANVAS_WIDTH);
  // рисуем черный квадрат размером 100 на 100 и по координатам
  ctx.fillRect(x, 50, 100, 100);
  // увеличиваем x для того, чтоб двигать квадрат
//   x++;
  // указываем браузера что проводим анимацию и просим запланировать перерисовку в след кадре анимации
  // параметр функция колббек будет вызвана, когда придет время обновить анимацию на след перерисовке
  let animId = requestAnimationFrame(animate);
  if (x === 601) {
    cancelAnimationFrame(animId);
  }
}
animate();

